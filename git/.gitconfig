[user]
	name = Paul Ellis
[core]
	excludesfile = ~/.gitignore_global
	ignorecase = false
[pull]
	rebase = true
[alias]
	graph = log --all --decorate --oneline --graph
	pull-all = pull --all -p
	soft-reset = reset --soft
	hard-reset = reset --hard
	master = checkout master
	develop = checkout develop
	dev = checkout develop
	statys = status
	st = status
	ch = checkout
